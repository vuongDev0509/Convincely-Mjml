        </mj-attributes>
        <mj-style>
            body{
                background: #EDF5FE;
            }
            body table tr td, h1, h2, h3, h4, h5, h6{
                margin:0;
                padding:0;
            }
            *{
                font-family: 'Montserrat', sans-serif;
            }
            .bt-heading span{
                font-weight: 400;
            }
            .heading-bold span{
                font-weight:700;
            }
            .bt-text-acn{
                
            }
            .bt-text-acn p:first-child{
                font-size: 30px;
                line-height: 100%;
                font-weight: 500;
                width: 90px;
                color: #404040;
                float: left;
                width: 57px;
                margin-right: 7px;
                margin-top: 2px;
            }
            .bt-text-acn p:last-child{
                font-size: 14px;
                line-height: 120%;
                color: #404040;
                letter-spacing: -0.01em;
                width: calc(100% - 64px);
            }
            .bt-text-acn p:last-child span{
                font-weight: 800;
            }
            .bt-text-acn p{
                margin:0;
                padding:0;
                display: inline-block;               
            }
            .bt-btn{
                text-align:center;
            }
            .bt-btn > a{
                display: inline-block;
                letter-spacing: -0.01em;
                font-weight: 700;
                font-size: 15px;
                line-height: 125%;
                color: #FFFFFF;
                background-color: #F67926;
                text-decoration: none;
                padding: 15px 37px 15px 23px;
                position: relative;
            }
            .bt-btn > a:before{
                content: " ";
                display: inline-block;
                width: 9px;
                height: 15px;
                background-repeat: no-repeat;
                background-size: cover;
                background-position: center;
                background-image: url("./assets/images/right arrow.png");
                position: absolute;
                top: 50%;
                transform: translateY(-50%);
                right: 18px;
            }
            .bt-price{
                width: 100%;
                
            }
            .bt-price .item{
                width: 50%;
                float: left;
            }
            .bt-price .item:last-child ._price{
                display: inline;
            }
            .bt-price .item:first-child{
              
            }
            .bt-price .item:last-child{
                text-align: right;
                -webkit-box-shadow: inset 1px 0px 0px rgba(64, 64, 64, 0.2);
                -moz-box-shadow: inset 1px 0px 0px rgba(64, 64, 64, 0.2);
                box-shadow: inset 1px 0px 0px rgba(64, 64, 64, 0.2);
            }
            .bt-price .item p{
                margin:0;
                padding:0;
               
            }
            .bt-price .item ._price{
                color:#157EF4;
                font-size: 24px;
                font-weight: 700;
                line-height: 110%;
                position: relative;
            }
            .bt-price .item:first-child ._price{
                padding-left: 20px;
            }
            .bt-price .item:last-child ._sub-price{
                padding-left: 0px;
                text-align: right;
            }
            .bt-price .item ._price:before{
                content: " ";
                display: inline-block;
                background-size: cover;
                background-repeat: no-repeat;
                background-position: center;
                position: absolute;
                top:0;
            }
            .bt-price .item:first-child ._price:before{
                background-image: url("./assets/images/Vector 12.svg");
                width:18px;
                height: 24px;
                left:-4px;
            }
            .bt-price .item:last-child ._price:before{
                background-image: url("./assets/images/Vector 11.svg");
                width:17px;
                height: 24px;
                left: -22px;
            }
            .bt-price .item ._price > span{
                font-size: 17px;
            }
            .bt-price .item ._price sup{
                font-size: 14px;
                font-weight: 600;
            }
            .bt-price .item ._sub-price{
                color: #404040;
                font-size: 13px;
                line-height: 110%;
                display:inline-block;
                width: 100%;
                text-align:center;
                margin: 2px 0 0 0;
            }
            .list-acn ._item-acn:not(:last-child){
                margin-bottom: 8px;
            }
            ._item-acn > img{
                margin-right: 8px;
                float: left;
            }
            ._item-acn ._text{
                font-size: 12px;
                line-height: 120%;
                letter-spacing: -0.02em;
                font-weight: 400;
                color: #404040;
            }
            ._item-acn ._text > b{
                 font-weight: 700;
            }
            a._underlined{
                text-decoration: underline;
                color: inherit;
                font-size: inherit;
                line-height: inherit;
                font-weight: inherit;
            }
            .column-border > table > tbody > tr > td > table > tbody{
                border: 1px solid rgba(64, 64, 64, 0.2);
                border-bottom-color: rgba(64, 64, 64, 0.6);
            }
            ._text-border-bottom{
                 border-bottom: 1px solid rgba(64, 64, 64, 0.2);
            }
            .column-not-border-bottom.column-border > table > tbody > tr > td > table > tbody{
                border-bottom: none;
            }
             .column-not-border-top.column-border > table > tbody > tr > td > table > tbody{
                 border-top: none;
             }
            ._text-between b > span{
                font-weight: 400;
            }
            ._text-between > div > span{
                float: right;
            }
            ._border-text > div{
                padding-bottom: 8px;
                border-bottom: 1px solid rgba(31, 75, 176, 0.12);
                position: relative;
            }
            ._border-text-02._border-text > div{
                padding-bottom: 12px;
            }
            ._text-footer > div{
                float: right;
            }
            _text-footer a{
                color: inherit !important;
            }
            @media only screen and (max-width: 599px){
                .heading-ss-hero > div{
                    font-size: 20px !important;
                    line-height: 25px !important
                }
                ._column-no-padding-mobi > table > tbody > tr > td{
                    padding: 0 !important;
                }

                ._column-no-padding-mobi:last-child{
                    margin-top: 15px;
                } 
                .section-main .mj-column-per-50{
                    width: 100% !important;
                    max-width: 400px;
                }
                .bt-text-acn p:first-child{
                       width: 57px;
                       margin-right: 10px;
                }
                .bt-text-acn p:last-child{
                    width: calc(100% - 67px);
                    max-width: 208px;
                }
                .bt-price .item:first-child ._sub-price{
                    text-align: left;
                    padding-left: 30px; 
                }
                .bt-price .item:last-child ._sub-price{
                    text-align: right;
                    padding-right: 13px;
                    padding-left: 0px;
                }
                .section-hero{
                    max-width: 430px !important;
                }
            }
            @media only screen and (max-width: 479px){
                .bt-header > table > tbody > tr > td > div{
                    width: 50% !important;
                    float: left;
                }
                .logo-footer > table{
                    margin-left: auto;
                    margin-right: auto;
                }
                ._text-footer{
                    width: 100%;
                    text-align: center;
                }
                ._text-footer>div{
                    margin-top: 15px;
                    display: inline-block;
                    margin-left: auto;
                    margin-right: auto;
                    float: inherit;
                    text-align: left;
                }               
            }
